{
  "title": "aria-roledescription attribute with HTML section",
  "description": "This test ensures that the value of aria-roledescription is announced by screen readers",
  "assertions": [
    {
      "feature_id": "aria/aria-roledescription_attribute",
      "feature_assertion_id": "convey_value",
      "css_target": "#target",
      "results": {
        "jaws": {
          "browsers": {
            "chrome": {
              "output": [
                {
                  "command": "next_item",
                  "from": "before target",
                  "to": "target",
                  "output": "\"target\"",
                  "result": "fail",
                  "notes": "no information about the section was conveyed"
                },
                {
                  "command": "open_element_list",
                  "output": "\"section name, region\"",
                  "result": "fail",
                  "notes": "in list of regions, but role was not changed."
                }
              ]
            },
            "firefox": {
              "output": [
                {
                  "command": "next_item",
                  "from": "before target",
                  "to": "target",
                  "output": "\"target\"",
                  "result": "fail",
                  "notes": "no information about the section was conveyed"
                },
                {
                  "command": "open_element_list",
                  "output": "\"section name, region\"",
                  "result": "fail",
                  "notes": "in list of regions, but role was not changed."
                }
              ]
            },
            "ie": {
              "output": [
                {
                  "command": "next_item",
                  "from": "before target",
                  "to": "target",
                  "output": "\"target\"",
                  "result": "fail",
                  "notes": "no information about the section was conveyed"
                },
                {
                  "command": "open_element_list",
                  "output": "\"no regions were found on the page\"",
                  "result": "fail"
                }
              ]
            }
          }
        },
        "narrator": {
          "browsers": {
            "edge": {
              "output": [
                {
                  "command": "next_item",
                  "from": "before target",
                  "to": "target",
                  "output": "\"Target\"",
                  "result": "fail"
                },
                {
                  "command": "next_landmark",
                  "from": "before target",
                  "to": "target",
                  "output": "\"section name, custom section role, region landmark\"",
                  "result": "pass"
                }
              ]
            }
          }
        },
        "nvda": {
          "browsers": {
            "chrome": {
              "output": [
                {
                  "command": "next_item",
                  "from": "before target",
                  "to": "target",
                  "output": "\"section name, custom section role, target.\"",
                  "result": "pass"
                }
              ]
            },
            "firefox": {
              "output": [
                {
                  "command": "next_item",
                  "from": "before target",
                  "to": "target",
                  "output": "\"Section name, custom section role, target.\"",
                  "result": "pass"
                }
              ]
            }
          }
        },
        "orca": {
          "browsers": {
            "firefox": {
              "output": [
                {
                  "command": "next_item",
                  "from": "before target",
                  "to": "target",
                  "output": "\"target, custom section role\"",
                  "result": "pass"
                }
              ]
            }
          }
        },
        "talkback": {
          "browsers": {
            "and_chr": {
              "output": [
                {
                  "command": "next_item",
                  "from": "before target",
                  "to": "target",
                  "output": "\"section name, region\"",
                  "result": "fail",
                  "notes": "Contents of the section could not be announced."
                },
                {
                  "command": "next_landmark",
                  "from": "before target",
                  "to": "target",
                  "output": "\"section name, region\"",
                  "result": "fail",
                  "notes": "Contents of the section could not be announced."
                }
              ]
            }
          }
        },
        "vo_macos": {
          "browsers": {
            "safari": {
              "output": [
                {
                  "command": "next_item",
                  "from": "before target",
