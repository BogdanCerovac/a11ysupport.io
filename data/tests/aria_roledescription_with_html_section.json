              ]
            }
          }
        },
        "vo_ios": {
          "browsers": {
            "ios_saf": {
              "output": [
                {
                  "command": "next_item",
                  "from": "before target",
                  "to": "target",
                  "output": "\"Target, section name, landmark\"",
                  "result": "fail"
                }
              ]
            }
          }
        }
      }
    }
